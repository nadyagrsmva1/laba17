<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="198.0" prefWidth="305.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab17.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="myLabel" alignment="CENTER" prefHeight="54.0" prefWidth="210.0" text="Число: 0" textAlignment="CENTER">
      <font>
         <Font size="15.0" />
      </font></Label>
   <Button fx:id="buttonPlus" mnemonicParsing="false" onAction="#ClickPlus" prefHeight="25.0" prefWidth="46.0" text="+1" />
    <Button fx:id="buttonMinus" mnemonicParsing="false" onAction="#ClickMinus" prefHeight="25.0" prefWidth="46.0" text="-1" />
</VBox>
