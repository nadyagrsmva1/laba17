<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="198.0" prefWidth="305.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab17.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="a">
      <graphic>
         <TextField fx:id="textA" alignment="TOP_LEFT" prefHeight="25.0" prefWidth="46.0" />
      </graphic>
   </Label>
   <Label layoutX="134.0" layoutY="52.0" text="b">
      <graphic>
         <TextField fx:id="textB" alignment="TOP_LEFT" prefHeight="25.0" prefWidth="46.0" />
      </graphic>
   </Label>
   <Label layoutX="134.0" layoutY="75.0" text="sum">
      <graphic>
         <TextField fx:id="textSum" alignment="TOP_LEFT" prefHeight="25.0" prefWidth="46.0" />
      </graphic>
   </Label>
   <Button fx:id="calculate" mnemonicParsing="false" onAction="#ClickCalculate" text="calculate" />
    <Button fx:id="clear" mnemonicParsing="false" onAction="#ClickClear" text="clear" />
</VBox>
